function getParameter ( parameterName ) {
  var queryString = window.location.search
  var parameterName = parameterName + "=";
  if ( queryString.length > 0 ) {
    begin = queryString.indexOf ( parameterName );
    if ( begin != -1 ) {
      begin += parameterName.length;
      end = queryString.indexOf ( "&" , begin );
      if ( end == -1 ) {
        end = queryString.length
      }
    return unescape ( queryString.substring ( begin, end ) );
    }
    return "null";
  }
}

if(document.URL.indexOf('ekoloko.mako.', 0) >= 0) {
  var url = "http://www.ekoloko.com/ekoloko/login.html";
  url = addUrlParam("language", url);       
  url = addUrlParam("username", url);       
  url = addUrlParam("friend", url);       
  url = addUrlParam("refCode", url);       
  url = addUrlParam("directLogin", url);      
  document.location.replace(url);
}


function popUp(URL) {
	var open_link = window.open('','_blank');
	open_link.location=URL;
	//window.open(URL, 'ekoloko', 'toolbar=0,scrollbars=1,location=0,statusbar=0,menubar=0,resizable=0,width=960,height=800')
}

function getVersion() {
	return Math.random();
}

function addUrlParam(pName, url) {
  var pValue = getParameter(pName);
  if(!( pValue == undefined || pValue == "undefined" || pValue == null || pValue == "null") ) {
    url += (url.indexOf('?')<0?"?":"&")+pName+'='+pValue;
  }
  return url;
}

var swfUrl = 'shell?version='+getVersion()
swfUrl = addUrlParam("register", swfUrl);       
swfUrl = addUrlParam("language", swfUrl);       
swfUrl = addUrlParam("username", swfUrl);       
swfUrl = addUrlParam("directLogin", swfUrl);       
swfUrl = addUrlParam("advisor", swfUrl);
swfUrl = addUrlParam("friend", swfUrl);
swfUrl = addUrlParam("refCode", swfUrl);

function createOnUnloadFunction() {
  window.onbeforeunload = exitConfirmation;  
}

function exitConfirmation(e) {

}          


function loadBanner(bannerId, url) { 
   bannerLoaded = true;
   var iframe = getBannerIframe(bannerId);
   if(iframe != null) {
      iframe.src = url;
   }
}

function getBannerIframe(bannerId) {
  var td = document.getElementById(bannerId+"TD");
  if(td) {
    var iframe = document.getElementById("banner"+bannerId);
    if(!iframe) {
      iframe = document.createElement('iframe');
      iframe.width = (bannerId=="top"||bannerId=="bottom")?960:180;
      iframe.height = (bannerId=="top"||bannerId=="bottom")?120:600;
      iframe.border = "0";
      iframe.frameBorder = "0";
      iframe.scrolling = "no";
      iframe.id = "banner"+bannerId;
      
      td.appendChild(iframe);            
    }
    return iframe;
  }
  return null;
}

var adsHorizontalUrl = [/*"./adsenseHorizontal_180.html","./adsenseHorizontal_200.html",*/"./adsenseHorizontal_728.html"];
var adsLeftUrl = "./adsenseLeft.html";
var adsLeftRightUrl = "./adsenseLeftRight.html";
var adsRightUrl = "./adsenseRight.html";
var adsRightLeftUrl = "./adsenseRightLeft.html";
var noAdsRightUrl = "./adsenseEmpty.html";
var maxad = 99999;//60 + Math.floor((Math.random() * 90));
var adnum = 0;
var bannerLoaded = false;

function hideAds() {
  var adsTable = document.getElementById("adBanners");
  if(adsTable) {
    adsTable.style.display="none";
  }
}

function setAdds(visible) {                
  var adsTable = document.getElementById("adBanners");
  if(adsTable) {
    adsTable.style.display="none";
    try{
      adsTable.innerHTML = "";
    } catch(e) {}
  }
  if(visible==0) {
    adsHorizontalUrl = null;
  } else {
    refreshAdsenseFrame();
  }
}

function refreshAdsenseFrame() {
  adnum ++;
  if(adnum < maxad)  {
    var adsFrame = document.getElementById("adBannersFrame");
    var adsLeftFrame = getBannerIframe("left");
    var adsRightFrame = getBannerIframe("right");
    if(adsHorizontalUrl != null) {
      if(adsFrame || adsLeftFrame) {
        var interval = 75+Math.floor(Math.random() * 75);
        setTimeout(function(){refreshAdsenseFrame();},(interval*1000));
        if(adsFrame) {
          var hUrl = adsHorizontalUrl[Math.floor(Math.random()*adsHorizontalUrl.length)];
          adsFrame.src = hUrl+"?language="+lang+"&v="+Math.random();      
        } 
        if(adsLeftFrame) {
          adsLeftFrame.src = ((bannerLoaded || adnum>4)?adsLeftRightUrl:adsLeftUrl)+"?language="+lang+"&v="+Math.random();          
        }
        if(adsRightFrame) {
          adsRightFrame.src = ((bannerLoaded || adnum>4)?adsRightLeftUrl:adsRightUrl)+"?language="+lang+"&v="+Math.random();          
        }        
        /*if(!document.getElementById("rightTD").src) {
          var adsRightFrame = getBannerIframe("right");
          adsRightFrame.src = noAdsRightUrl;    
        } */
      } 
    }
  }
}

//IMA3 functions
function removeAdSwf() {
  removeVideoAd();
}

function noAdsReturned() {
  removeVideoAd();
}

function getFlashMovieObject(movieName){
	if (window.document[movieName]){
		return window.document[movieName];
	}
	if (navigator.appName.indexOf("Microsoft Internet")==-1){
		if (document.embeds && document.embeds[movieName])
			return document.embeds[movieName];
	}
	
	return document.getElementById(movieName);
  	
}
  
function removeVideoAd() {       
  var flash = getFlashMovieObject("shell");
  if(!flash.removeAd) {                     
    flash = getFlashMovieObject("shell");
  }
  // call action script function
  flash.removeAd();
}


//Set default language
var lang = getParameter('language');
if (!lang || lang == "null") lang = "iw"; 

function sendGoogleActionRequest(module, activity, label) {
  if(label==null || label==undefined || label == "null" || label =="undefined") label="";
	_vtq._trackEvent(module, activity, ""+label);
}

 
